{
  "name": "the-new-fuse",
  "version": "2.1.0",
  "description": "The New Fuse - AI Agency Platform with Multi-Tenant Architecture",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*",
    "packages/port-management",
    "tools/port-manager",
    "chrome-extension"
  ],
  "scripts": {
    "build": "turbo run build",
    "build:api": "turbo run build --filter=@the-new-fuse/api-server",
    "build:chrome": "yarn workspace the-new-fuse-chrome-extension run build",
    "build:all": "turbo run build && yarn build:chrome",
    "dev": "turbo run dev",
    "dev:api": "turbo run dev --filter=@the-new-fuse/api-server",
    "dev:chrome": "yarn workspace the-new-fuse-chrome-extension run dev",
    "start": "turbo run start",
    "start:api": "turbo run start --filter=@the-new-fuse/api-server",
    "test": "turbo run test",
    "test:chrome": "yarn workspace the-new-fuse-chrome-extension run test",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "clean": "turbo run clean",
    "clean:chrome": "yarn workspace the-new-fuse-chrome-extension run clean",
    "db:generate": "turbo run db:generate",
    "db:migrate": "turbo run db:migrate",
    "mcp:build": "./build-mcp.sh",
    "mcp:dev": "./dev-mcp.sh",
    "mcp:start": "./run-mcp-local.sh",
    "mcp:start:remote": "./run-mcp-remote.sh",
    "mcp:test": "./test-mcp.sh",
    "ports:status": "tnf-ports status",
    "ports:conflicts": "tnf-ports conflicts",
    "ports:dev": "tnf-ports dev --optimize",
    "ports:health": "tnf-ports health",
    "dev:optimized": "yarn ports:dev && yarn dev",
    "package:chrome": "yarn workspace the-new-fuse-chrome-extension run package:extension",
    "release:chrome": "yarn workspace the-new-fuse-chrome-extension run build:package"
  },
  "keywords": [
    "ai-agency",
    "multi-tenant",
    "nestjs",
    "agency-hub",
    "swarm-orchestration",
    "typescript"
  ],
  "author": "The New Fuse Team",
  "license": "MIT",
  "devDependencies": {
    "@the-new-fuse/port-management": "workspace:*",
    "@the-new-fuse/port-manager-cli": "workspace:*",
    "@turbo/gen": "^2.5.3",
    "eslint": "^8.0.0",
    "html-webpack-plugin": "^5.6.0",
    "prisma": "6.8.2",
    "turbo": "^2.5.3",
    "typescript": "^5.8.3",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4"
  },
  "packageManager": "yarn@4.9.1+sha512.f95ce356460e05be48d66401c1ae64ef84d163dd689964962c6888a9810865e39097a5e9de748876c2e0bf89b232d583c33982773e9903ae7a76257270986538",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.6.0",
    "@prisma/client": "6.8.2",
    "cross-env": "^7.0.3",
    "express": "^4.21.2",
    "zod": "^3.22.4"
  },
  "resolutions": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/axios": "^3.0.0",
    "@nestjs/mongoose": "^10.0.0",
    "@nestjs/swagger": "^7.0.0",
    "@nestjs/testing": "^10.0.0",
    "@nestjs/event-emitter": "^2.0.0",
    "@nestjs/websockets": "^10.0.0",
    "@nestjs/platform-socket.io": "^10.0.0",
    "@types/react": "^18.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "reflect-metadata": "^0.1.14",
    "rxjs": "^7.8.1",
    "vite": "^5.0.0",
    "rimraf": "^5.0.5",
    "@testing-library/react-hooks": "^8.0.1"
  }
}
