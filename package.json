{
  "name": "the-new-fuse",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "packageManager": "yarn@4.10.0",
  "workspaces": {
    "packages": [
      "apps/*",
      "packages/*",
      "src/vscode-extension",
      "chrome-extension"
    ]
  },
  "scripts": {
    "prebuild": "node ./scripts/kill-port-processes.js",
    "build": "yarn workspaces foreach -Apt run build",
    "build:production": "NODE_ENV=production yarn workspaces foreach -Apt run build",
    "build:docker": "bash ./scripts/docker-build.sh",
    "build:production:full": "bash ./scripts/production-build.sh",
    "check:docker-ports": "node ./scripts/check-docker-ports.js",
    "kill-ports": "node ./scripts/kill-port-processes.js",
    "dev": "yarn kill-ports && yarn workspaces foreach -Apiv run dev",
    "lint": "yarn workspaces foreach -Apt run lint",
    "test": "yarn workspaces foreach -Apt run test",
    "clean": "yarn workspaces foreach -Apt run clean",
    "clean:build": "rimraf '**/dist' '**/build' '**/out' '**/lib' '**/.next' '**/coverage' '**/*.tsbuildinfo' '.turbo'",
    "clean:deps": "rimraf '**/node_modules'",
    "clean:cache": "yarn cache clean && rimraf .yarn/{cache,build-state.yml,install-state.gz} .parcel-cache .cache",
    "clean:docker": "bash ./scripts/docker-cleanup.sh",
    "clean:all": "bash ./scripts/cleanup.sh",
    "prepare": "cp .env.example .env",
    "start:redis-monitor": "tsx src/scripts/redisChannelMonitor.ts"
  },
