# A2A Protocol Configuration
A2A_PROTOCOL_VERSION=1.0
A2A_SECURITY_KEY=your-secret-key
A2A_TIMEOUT=30000
A2A_RETRY_ATTEMPTS=3

# Endpoints
A2A_PRIMARY_ENDPOINT=http://localhost:3000
A2A_FALLBACK_ENDPOINT=http://localhost:3001

# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_NAME=a2a_metrics
DB_USER=newfuse
DB_PASSWORD=secretpass

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# Monitoring
METRICS_RETENTION_DAYS=30
ENABLE_DETAILED_LOGGING=true