{
  "name": "the-new-fuse",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "bash scripts/dev-launch.sh",
    "start": "bash scripts/launch-prod.sh",
    "build": "turbo run build",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "clean": "node scripts/cleanup.mjs --all",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "build:types": "tsc --emitDeclarationOnly",
    "build:ts": "tsc --build",
    "clean:deps": "node scripts/cleanup.mjs --deps",
    "clean:build": "node scripts/cleanup.mjs --build",
    "clean:docker": "node scripts/cleanup.mjs --docker",
    "clean:cache": "node scripts/cleanup.mjs --cache",
    "prepare": "cp .env.example .env"
  },
  "packageManager": "yarn@3.6.0",
  "dependencies": {
    "@chakra-ui/react": "^2.8.0",
    "@firebase/analytics": "^0.10.12",
