    "@firebase/remote-config-compat": "^0.2.13",
    "@firebase/storage": "^0.13.7",
    "@firebase/storage-compat": "^0.3.17",
    "@nestjs/common": "^11.0.11",
    "@nestjs/config": "^4.0.1",
    "@nestjs/core": "^11.0.11",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/platform-express": "^11.0.11",
