    "@types/uuid": "^10.0.0",
    "bcrypt": "^5.1.1",
    "firebase": "^11.4.0",
    "joi": "^17.13.3",
    "pg": "^8.13.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "typeorm": "^0.3.21",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
