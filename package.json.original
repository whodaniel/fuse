{
  "name": "the-new-fuse",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "clean": "yarn workspaces foreach -p run clean",
    "build": "yarn workspaces foreach -p run build",
    "dev": "yarn workspaces foreach -p run dev",
    "build:core": "yarn workspace @the-new-fuse/core build",
    "build:database": "yarn workspace @the-new-fuse/database build",
    "build:types": "tsc -p tsconfig.types.json",
    "clean:deps": "yarn workspaces foreach -ptv run clean && rimraf .yarn/cache && yarn cache clean",
    "test": "yarn workspaces foreach -ptv run test",
    "lint": "yarn workspaces foreach -ptv run lint",
    "type-check": "yarn workspaces foreach -ptv run type-check",
    "verify:deps": "yarn why tensorflow || echo 'No TensorFlow dependencies found'",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "prepare": "cp .env.example .env",
    "update-deps": "node scripts/update-dependencies.js",
    "start": "node dist/index.js",
    "fix-types": "bash install-deps.sh"
  },
  "resolutions": {
